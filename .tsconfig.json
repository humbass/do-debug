{
  "compilerOptions": {
    "target": "ESNEXT",
    "module": "ESNext",
    "lib": ["es2019", "dom"],
    "jsx": "react",
    "declaration": false,
    "sourceMap": false,
    "outDir": "./dist",
    "moduleResolution": "node",
    "baseUrl": "./",
    "paths": {
      "core-js/es6/*": [
        "node_modules/core-js/es/*"
      ],
      "@localTypes/*": ["types/*"],
      "@vue/*": ["lib/loader/hippy-vue/src/*"],
      "@router/*": ["lib/loader/hippy-vue-router/src/*"],
      "@css-loader/*": ["lib/loader/hippy-vue-css-loader/src/*"],
      "@native-components/*": ["lib/loader/hippy-vue-native-components/src/*"],
      "vue": ["node_modules/vue/src/core/index"],
      "compiler/*": ["node_modules/vue/src/compiler/*"],
      "web/*": ["node_modules/vue/src/platforms/web/*"],
      "core/*": ["node_modules/vue/src/core/*"],
      "shared/*": ["node_modules/vue/src/shared/*"],
      "sfc/*": ["node_modules/vue/src/sfc/*"],
      "he": ["lib/loader/hippy-vue/src/util/entity-decoder"]
    },
    "esModuleInterop": true
  }
}
